{
  "name": "circom-ed25519",
  "version": "0.0.1",
  "description": "Circom circuits for ed25519 signature verification",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha --colors --max-old-space-size=4000 --timeout 150000 --exclude test/inversemodulo* --exclude test/scalarmul* --exclude test/ed25519*",
    "test-inverse-modulo": "mocha --colors --max-old-space-size=4000 --timeout 150000 test/inversemodulo.test.js",
    "test-scalarmul": "mocha --colors --max-old-space-size=4000 --timeout 200000 test/scalarmul.test.js",
    "test-verify": "mocha --colors --max-old-space-size=4000 --timeout 1000000 test/ed25519verfication.test.js ",
    "clean": "rm -rf node_modules && make clean && npm i"
  },
  "keywords": [
    "circuit",
    "circom",
    "zksnark"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Electron-Labs/circom-ed25519.git"
  },
  "author": "rahulghangas, garvitgoel, BakerStreetPhantom, suniljalandhra",
  "devDependencies": {
    "bigint-mod-arith": "^3.0.2",
    "chai": "^4.3.6",
    "circom_tester": "0.0.9",
    "circomlibjs": "^0.1.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "fast-check": "^2.22.0",
    "mocha": "^9.2.1",
    "mocha-logger": "^1.0.7"
  }
}
